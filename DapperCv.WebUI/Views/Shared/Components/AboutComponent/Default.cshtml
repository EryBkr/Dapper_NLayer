@model AppUserListDto

<section class="resume-section" id="about">
    <div class="resume-section-content">
        <h1 class="mb-0">
            @Model.FirstName
            <span class="text-primary">@Model.LastName</span>
        </h1>
        <div class="subheading mb-5">
            @Model.Address ·
            <a href="mailto:@Model.Email">@Model.Email<a>
        </div>
        <p class="lead mb-5">@Model.ShortDescription .</p>
        @* Ekstradan lazım olan datalar için ViewComponent içerisinde TagHelper Kullandık *@
        <getIcons user-id=1></getIcons>
    </div>
</section>